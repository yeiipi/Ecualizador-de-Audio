function [B] = bandas(frec,b)
%UNTITLED2 Summary of this function goes here
%   Detailed explanation goes here
l = length(frec)/2;
l_banda = floor(l/b);

B = []';
inicio = 1;
for i=1:b
    B = [B; frec(inicio:(inicio + l_banda))]; 
    inicio = inicio + l_banda;
end 

disp("og len vs new len")
disp(length(frec))
disp(length(reshape(B',1,[])))
    
end

